    "\n",
    "from scipy.optimize import fsolve\n",
    " \n",
    "def solve_function(unsolved_value):\n",
    "    x,y=unsolved_value[0],unsolved_value[1]\n",
    "    return [\n",
    "        x*x+2*x*y,\n",
    "        2*x*y-2*y*y\n",
    "    ]\n",
    " \n",
    "solved=fsolve(solve_function,[6, -3])\n",
    "print(solved)\n",
    " \n",
    "print(\"Program done!\")"
   ]
